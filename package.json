{
  "name": "room302-template",
  "version": "1.1.2",
  "description": "A CLI for creating new projects from the room302 template",
  "main": "index.js",
  "bin": {
    "room302-template": "./index.js"
  },
  "dependencies": {
    "inquirer": "^7.3.3",
    "ora": "^7.0.1",
    "shelljs": "^0.8.4"
  },
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.x.x",
    "@babel/plugin-transform-modules-commonjs": "^7.x.x",
    "@jest/globals": "^29.x.x",
    "jest": "^29.7.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "type": "commonjs"
}
